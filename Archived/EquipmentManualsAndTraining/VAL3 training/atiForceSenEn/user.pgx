  userPage()
  do
    cls()
    put("status: ")
    putln(nStatus)
    put("fX: ")
    putln(nForce[0])
    put("fY: ")
    putln(nForce[1])
    put("fZ: ")
    putln(nForce[2])
    put("mX: ")
    putln(nForce[3])
    put("mY: ")
    putln(nForce[4])
    put("mZ: ")
    putln(nForce[5])
    gotoxy(0,13)
    put("ZERO")
    l_nKey=getKey()
    switch l_nKey
      case 271
        bZero=true
      break
    endSwitch
    delay(0)
  until false
end

      </code>
