<?xml version="1.0" encoding="utf-8" ?>
<programList xmlns="ProgramNameSpace" >
  <program name="main" public="false" >
    <description />
    <paramSection/>
    <localSection/>
    <source>
      <code>begin
  while bQuit==false
    call home()
    if diRun==true
      doRunning=true
      nTimeStart=clock()
      call runCycle()
      nTime=clock()-nTimeStart
      nCount=nCount+1
    else
      doRunning=false
    endIf
    delay(0)
  endWhile
end

      
      </code>
    </source>
  </program>
</programList>